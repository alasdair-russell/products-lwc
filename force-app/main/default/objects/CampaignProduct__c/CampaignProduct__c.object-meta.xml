<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
  <allowInChatterGroups>false</allowInChatterGroups>
  <deploymentStatus>Deployed</deploymentStatus>
  <enableActivities>false</enableActivities>
  <enableBulkApi>true</enableBulkApi>
  <enableHistory>false</enableHistory>
  <enableReports>true</enableReports>
  <enableSearch>true</enableSearch>
  <enableSharing>true</enableSharing>
  <externalSharingModel>Private</externalSharingModel>
  <label>Campaign Product</label>
  <pluralLabel>Campaign Products</pluralLabel>
  <nameField>
    <label>Campaign Product Name</label>
    <type>AutoNumber</type>
    <displayFormat>CP-{0000}</displayFormat>
  </nameField>
  <sharingModel>ReadWrite</sharingModel>
  <validationRules>
    <fullName>Require_Product</fullName>
    <active>true</active>
    <description>Require Product__c to be filled</description>
    <errorConditionFormula>ISBLANK(Product__c)</errorConditionFormula>
    <errorDisplayField>Product__c</errorDisplayField>
    <errorMessage>Product is required.</errorMessage>
  </validationRules>
</CustomObject>
